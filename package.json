{
  "name": "eterna-tooling",
  "version": "0.0.1",
  "description": "Shared Tooling Configurations",
  "homepage": "https://github.com/nickstaroba/eterna-tooling#readme",
  "bugs": {
    "url": "https://github.com/nickstaroba/eterna-tooling/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickstaroba/eterna-tooling.git"
  },
  "license": "MIT",
  "author": "Nick Staroba",
  "workspaces": [
    "./packages/*",
    "./src/*"
  ],
  "scripts": {
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix --resolve-plugins-relative-to ./",
    "lint:prettier": "prettier \"./**/*.{css,html,js,json,jsx,md,scss,ts,tsx}\" --write",
    "packages:build": "gulp build",
    "packages:publish-dry-run": "lerna exec \"npm publish --registry http://localhost:4873\"",
    "packages:registry": "verdaccio",
    "packages:update": "gulp update",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/register": "7.14.5",
    "@babel/runtime": "7.14.6",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "html-webpack-plugin": "5.3.2",
    "mini-css-extract-plugin": "2.1.0",
    "npm-check-updates": "11.8.3",
    "prettier": "2.3.2",
    "sort-package-json": "1.50.0",
    "typescript": "4.3.5",
    "webpack-merge": "5.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "eslint": "7.31.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.1",
    "lerna": "4.0.0",
    "lint-staged": "11.0.1",
    "verdaccio": "5.1.2"
  }
}
